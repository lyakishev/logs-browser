<?xml version="1.0"?>
<root>
        <queries>
	<query name="Дата сообщения, имя лога, тип" add_lid="1" default="0">
select date, logname, type from #this
order by date desc
	</query>

	<query name="Время сообщения, имя лога, тип" add_lid="1" default="0">
select strftime('%H:%M:%f',date), logname, type from #this
order by date desc
	</query>

	<query name="Дата сообщения, имя лога, тип; группировка по дате, логу, типу" add_lid="1" default="0">
select date, logname, type from #this
group by date, logname, type
order by date desc
	</query>

	<query name="Время сообщения, имя лога, тип; группировка по дате, логу, типу" add_lid="1" default="0">
select strftime('%H:%M:%f',date), logname, type from #this
group by date, logname, type
order by date desc
	</query>

	<query name="Группировка по идущим подряд именам логa" add_lid="1" default="1">
select strftime('%H:%M:%f',min(date)) as ftime, 
strftime('%H:%M:%f',max(date)) as totime,
logname, computer, error(type)
from (select date, logname, computer, type, group_logname(logname) as logname_group
      from #this
      group by date, logname, computer, type
      order by date DESC)
group by logname_group
order by logname_group desc
	</query>

	<query name="Количество сообщений по именам лога" add_lid="1" default="0">
select logname, computer, count(log) as c, error(type)
from #this
group by logname, computer
order by c desc
	</query>

	<query name="Количество сообщений" add_lid="1" default="0">
select count(*)
from #this
	</query>

<query name="Ошибки" add_lid="1" default="1">
select strftime('%H:%M:%f',date), logname, type from #this
where type = 'ERROR'
order by date desc
</query>
</queries>
